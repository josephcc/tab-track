{
  "manifest_version": 2,
  "name": "Tab Tracker",
  "key": "pjlloikackppnoiffdfomhjeoejpigoi",
  "description": "Track tab usage",
  "version": "0.1",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "webNavigation",
    "chrome://favicon/",
    "*://report-tabs.cmusocial.com/*"
  ],
  "optional_permissions": [
    "alarms"
  ],
  "externally_connectable": {
    "matches": ["*://report-tabs.cmusocial.com/*", "*://localhost/*"]
  },
  "background": {
    "page": "html/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "img/structify_medium.png"
  },
  "icons": {
    "16": "img/structify_small.png",
    "48": "img/structify_medium.png",
    "128": "img/structify.png"
  }

}
