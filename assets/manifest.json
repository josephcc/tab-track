{
  "manifest_version": 2,

  "name": "Tab Tracker",
  "description": "Track tab usage",
  "version": "0.1",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "webNavigation",
    "sessions",
    "chrome://favicon/"
  ],
  "background": {
    "page": "html/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "img/structify_medium.png"
  },
  "icons": {
    "16": "img/structify_small.png",
    "48": "img/structify_medium.png",
    "128": "img/structify.png"
  }

}
